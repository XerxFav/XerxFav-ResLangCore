import subprocess
import re

PHASE_KEYWORDS = {
    "generation": ["generate", "create", "synthesize", "produce"],
    "interpretation": ["implement", "link", "refactor", "enforce"],
    "ontology": ["expand", "update", "define", "synchronize"],
    "ci": ["add", "integrate", "validate", "upload", "test"],
}

def classify_commit(message):
    message = message.lower()
    for phase, keywords in PHASE_KEYWORDS.items():
        if any(kw in message for kw in keywords):
            return phase
    return "unclassified"

def get_commits():
    result = subprocess.run(
        ["git", "log", "--pretty=format:%h %s"],
        capture_output=True, text=True
    )
    lines = result.stdout.strip().split("\n")
    return [line.split(" ", 1) for line in lines]

def build_phase_map():
    commits = get_commits()
    phase_map = {phase: [] for phase in PHASE_KEYWORDS}
    phase_map["unclassified"] = []

    for hash_, message in commits:
        phase = classify_commit(message)
        phase_map[phase].append((hash_, message))

    return phase_map

def display_map(phase_map):
    for phase, entries in phase_map.items():
        print(f"\nüîπ {phase.upper()} ({len(entries)} commits):")
        for hash_, msg in entries:
            print(f"  - {hash_}: {msg}")

if __name__ == "__main__":
    phase_map = build_phase_map()
    display_map(phase_map)

